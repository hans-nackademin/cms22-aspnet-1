@model CardViewModel

<div class="col">
    <div class="card h-100">
        <img src="@Model.ImageName" class="card-img-top" alt="@Model.Name">
        <div class="card-body">
            <h5 class="card-title">@Model.Name</h5>
            <p class="card-text">$@Model.Price</p>
        </div>
    </div>
</div>